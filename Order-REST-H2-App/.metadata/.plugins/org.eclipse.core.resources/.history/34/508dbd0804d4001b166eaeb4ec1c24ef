package groovydemo

class Patient {
	def firstName, lastName, age
	void setLastName(lastName) {
		if(lastName==null) {
			throw new IllegalArgumentException("Last Name can't be null");
		}
	}
}

p = new Patient(firstName: 'vasudeep',lastName: 'narra',age:64)

println p.firstName+" "+p.lastName+" "+p.age
